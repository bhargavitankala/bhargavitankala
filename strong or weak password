#include<stdio.h>
#include<string.h>
void main()
{
    char pass[30];
    printf("Enter your password:");
    scanf("%s",pass);
    int len=strlen(pass);
    int u=0,l=0,d=0,s=0;
    if(len>7)
    {
        for( int i=0;i<len;i++)
        {
             if(isupper(pass[i]))
                u++;
            else if(islower(pass[i]))
                l++;
            else if(isdigit(pass[i]))
                d++;
            else
                s++;
        }
        if(l>0&&u>0&&d>0&&s>0)
        printf("strong password");
        else
        printf("Weak");
    }
    else
    {

        printf("weak due to less characters");
    }


    }
